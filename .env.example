# Map Services Configuration
POSTGRES_PASSWORD=nominatim_password
REDIS_PASSWORD=redis_map_pass
NODE_ENV=development

# London boundaries (West, South, East, North)
LONDON_WEST=-0.52
LONDON_SOUTH=51.28
LONDON_EAST=0.33
LONDON_NORTH=51.72

# Service URLs for CONTAINERS (used by map-api service)
# These use Docker service names, not localhost
NOMINATIM_URL=http://nominatim-api:8080
OSRM_URL=http://osrm-backend:5000
REDIS_URL=redis://redis:6379

# Service URLs for HOST (if you need to access from outside Docker)
NOMINATIM_HOST_URL=http://localhost:8181
OSRM_HOST_URL=http://localhost:5001
REDIS_HOST_URL=redis://localhost:6380

# Optimization for local development
PG_SHARED_BUFFERS=1GB
PG_WORK_MEM=16MB
NOMINATIM_THREADS=2

# User permissions (optional, for file permissions)
USER_ID=1000
GROUP_ID=984